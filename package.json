{
  "name": "@omneedia/client-js",
  "version": "2.0.1",
  "description": "Isomorphic Javascript client for Omneedia",
  "keywords": [
    "javascript",
    "typescript",
    "omneedia",
    "client-js"
  ],
  "homepage": "https://github.com/omneedia/client-js",
  "bugs": "https://github.com/omneedia/client-js/issues",
  "license": "MIT",
  "author": "Omneedia",
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "dist/module/index.d.ts",
  "sideEffects": false,
  "repository": "omneedia/client-js",
  "scripts": {
    "clean": "rimraf dist docs",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "build": "yarn clean && yarn format && yarn build:main && yarn build:module && yarn build:umd",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "build:umd": "webpack",
    "types-generate": "dts-gen -m '@omneedia/client-js' -s",
    "dev:main": "tsc-watch -p tsconfig.json --onSuccess \"echo Main build successful\"",
    "dev:module": "tsc-watch -p tsconfig.module.json --onSuccess \"echo Module build successful\"",
    "dev:umd": "webpack --watch",
    "dev": "npm-run-all --parallel dev:main dev:module dev:umd"
  },
  "dependencies": {
    "@capacitor/browser": "^5.0.6",
    "@capacitor/core": "^5.2.3",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jsonwebtoken": "^9.0.0",
    "axios": "^1.7.7",
    "bufferutil": "^4.0.7",
    "capacitor": "^0.5.5",
    "cordova-plugin-inappbrowser": "^5.0.0",
    "cross-fetch": "^4.0.0",
    "encoding": "^0.1.13",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-search": "^0.13.3",
    "jsonwebtoken": "^9.0.0",
    "next": "^14.2.13",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-client": "^4.5.4",
    "utf-8-validate": "^6.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "husky": "^4.3.0",
    "jest": "^26.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "semantic-release-plugin-update-version-in-files": "^1.1.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.11",
    "tsc-watch": "^6.2.0",
    "typedoc": "^0.22.16",
    "typescript": "^4.5.5",
    "webpack": "^5.95.0",
    "webpack-cli": "^4.9.2"
  },
  "jsdelivr": "dist/umd/omneedia.js",
  "unpkg": "dist/umd/omneedia.js",
  "publishConfig": {
    "access": "public"
  }
}
